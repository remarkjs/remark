{
  "name": "mdast.com",
  "private": true,
  "version": "0.0.0",
  "description": "The mdast.com website",
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "dependencies": {
    "duo": "^0.15.0",
    "duo-babel": "^6.0.0"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "duo --use duo-babel --type js --stdout < demo/index.js > demo/build.js",
    "build:css": "duo --type css --stdout < index.css > build.css",
    "prepublish": "npm run build"
  }
}
