{
  "private": true,
  "license": "MIT",
  "homepage": "http://remark.js.org",
  "repository": "https://github.com/wooorm/remark",
  "bugs": "https://github.com/wooorm/remark/issues",
  "devDependencies": {
    "bail": "^1.0.0",
    "browserify": "^14.0.0",
    "bundle-collapser": "^1.2.1",
    "camelcase": "^4.0.0",
    "chalk": "^1.1.3",
    "clone": "^2.0.0",
    "escodegen": "^1.7.0",
    "esmangle": "^1.0.0",
    "esprima": "^3.1.0",
    "execa": "^0.6.0",
    "extend": "^3.0.0",
    "has": "^1.0.1",
    "lerna": "2.0.0-rc.4",
    "mdast-util-assert": "^1.0.0",
    "mdast-util-compact": "^1.0.0",
    "nyc": "^10.0.0",
    "remark-preset-wooorm": "^2.0.0",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "unist-builder": "^1.0.2",
    "unist-util-remove-position": "^1.1.0",
    "unist-util-visit": "^1.1.0",
    "vfile": "^2.0.0",
    "xo": "^0.18.0"
  },
  "scripts": {
    "postinstall": "lerna bootstrap",
    "build-md": "packages/remark-cli/cli.js . --frail --quiet",
    "build-bundle": "node script/build-bundle.js",
    "build": "npm run build-md && npm run build-bundle",
    "lint": "xo",
    "test-api": "node test/api",
    "test-api-extensive": "TEST_EXTENDED=true node test/api",
    "test-cli": "node test/cli.js",
    "test-coverage": "nyc --reporter lcov tape test",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": true,
    "rules": {
      "import/no-unassigned-import": "off",
      "unicorn/prefer-type-error": "off",
      "unicorn/escape-case": "off",
      "eqeqeq": "off",
      "guard-for-in": "off",
      "max-depth": "off",
      "complexity": "off",
      "no-eq-null": "off",
      "prefer-const": "off",
      "import/extensions": "off"
    },
    "ignores": [
      "remark.js"
    ]
  },
  "remarkConfig": {
    "plugins": ["preset-wooorm"]
  }
}
